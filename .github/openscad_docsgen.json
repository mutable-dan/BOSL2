{
    "problemMatcher": [
        {
            "owner": "docsgen",
            "pattern": [
                {
                    "regexp": "^!! ((WARNING|ERROR) at ([^:]+):(\\d+): .*[^:])$",
                    "message": 1,
                    "severity": 2,
                    "file": 3,
                    "line": 4
                }
            ]
        }
    ]
}
